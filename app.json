//app.json
{
  "expo": {
    "name": "expo-app",
    "slug": "expo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Fer.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Fer.png",
      "resizeMode": "contain",
      "backgroundColor": "#6c93f0"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Fer.png",
        "backgroundColor": "#6c93f0"
      }
    },

    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
